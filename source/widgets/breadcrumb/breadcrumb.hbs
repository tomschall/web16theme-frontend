<nav class="widg_breadcrumb {{#if entries}}{{else}}no-entries{{/if}}" data-init="breadcrumb">
  <ul>
		{{#each entries}}
      <li class="{{extraClasses}}">
				{{#if extraClasses}}
        	<a class="not-default" href="{{url}}" alt="{{title}}"></a>
				{{else}}
					{{#if lastBreadcrumb}}
						<p data-pt-title="{{title}}">{{title}}</p>
					{{else}}
					<a class="not-default {{#if lastBreadcrumb}}no-link{{/if}}" href="{{url}}" alt="{{title}}" data-pt-title="{{title}}">
						{{title}}
					</a>
					{{/if}}
				{{/if}}
      </li>
				{{#if lastBreadcrumb}}
				<li>
					<p style="background: red; width: 10px; height: 10px;"></p>
				</li>
			{{/if}}
		{{/each}}
  </ul>
  <span data-breadcrumb="extendstring" style="display: none" aria-hidden="true">{{langStrings.extend}}</span>
</nav>