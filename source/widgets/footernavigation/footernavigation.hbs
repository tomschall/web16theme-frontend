<div class="widg_footernavigation" data-init="footernavigation">
	{{#each entries}}
		<div class="widg_footernavigation__column" data-footernavigation="column">
			<span data-footernavigation="columnTitle">{{title}}</span>
			{{#if subentries}}
				<ul>
					{{#each subentries}}
						<li>
							<a href="{{url}}">{{title}}</a>
						</li>
					{{/each}}
				</ul>
			{{/if}}
		</div>
	{{/each}}
</div>
<div class="widg_footer__efqm" data-footernavigation="column">
	<a class="efqm" href="http://www.efqm.ch"></a>
	<span>Member of:</span>
	<a class="fhnw" href="https://www.swissuniversities.ch/"></a>
</div>
